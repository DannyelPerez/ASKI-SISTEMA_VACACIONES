@model  ASKI_VACACIONES.Models.DepartamentoModel



<section class="content-header">
    <h1>
        Departamentos

    </h1>
    <ol class="breadcrumb">
        <li><a href="~/Home/Calendario"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Departamentos</li>
    </ol>
</section>

<!-- Content Wrapper. Contains page content -->
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-default">
                @using (Html.BeginForm())
                {

                    <div class="panel-body">
                        <div class="box box-primary">
                            <div class="box-header with-border">
                                <h3 class="box-title"></h3>
                            </div><!-- /.box-header -->
                            <!-- form start -->
                            <form class="form-horizontal">

                                <ul class="nav nav-tabs">
                                    <li><a href="~/Usuarios/Index">Agregar</a></li>
                                    <li><a href="~/Usuarios/Edit">Modificar</a></li>
                                    <li class="active"><a data-toggle="tab" href="#0">Eliminar</a></li>
                                </ul>

                                <div class="tab-content">
                                    <div class="tab-pane fade in active">
                                        <div class="box-body">
                                            <div class="form-group">
                                                <label for="inputiddepartamento" class="col-sm-2 control-label">Id departamento</label>
                                                <div class="col-sm-10">
                                                    <p> <input type="text" class="NumberInteger" id="inputiddepartamento" size="20"></p>
                                                </div>
                                            </div>



                                            <div class="box-footer">
                                                <button type="submit" class="btn btn-info pull-right">Eliminar</button>
                                            </div><!-- /.box-footer -->
                                        </div>
                                    </div>
                                </div> <!-- cd-tabs-content -->
                            </form>
                        </div><!-- /.box -->
                    </div>

                }
            </div>

        </div>
    </div>

    <!-- Left col -->
    <section class="col-lg-7 connectedSortable"></section><!-- /.Left col -->
    <!-- right col (We are only adding the ID to make the widgets sortable)-->
    <section class="col-lg-5 connectedSortable"></section><!-- right col -->


</section><!-- /.content -->
