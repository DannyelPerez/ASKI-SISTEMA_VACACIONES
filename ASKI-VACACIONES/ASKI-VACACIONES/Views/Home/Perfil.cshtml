@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="content-header">
    <h1>
        Mi Perfil

    </h1>
    <ol class="breadcrumb">
        <li><a href="~/Home/Calendario"><i class="fa fa-dashboard"></i> Home</a></li>
        <li class="active">Mi Perfil</li>
    </ol>
</section>

<!-- Content Wrapper. Contains page content -->
<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-default">
                    <div class="panel-body">
                        <div class="box box-primary">
                            <div class="box-header with-border">
                                <h3 class="box-title"></h3>
                            </div><!-- /.box-header -->
                            <!-- form start -->
                            <form class="form-horizontal" id="formulario">

                             
                                            <div class="box-body">

                                                <div class="form-group">
                                                    <label for="inputtalentohumano" class="col-sm-2 control-label">Talento humano</label>
                                                    <div class="col-sm-10">
                                                        <input type="text" class="form-control" id="inputtalentohumano"> 
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="inputprimernombre" class="col-sm-2 control-label">Nombre</label>
                                                    <div class="col-sm-10">
                                                        <input type="text" class="form-control" id="inputprimernombre">
                                                    </div>
                                                </div>
                                              
                                                <div class="form-group">
                                                    <label for="inputapellido" class="col-sm-2 control-label">Apellido</label>
                                                    <div class="col-sm-10">
                                                        <input type="text" class="form-control" id="inputapellido">
                                                    </div>
                                                </div>
                   
                                                <div class="form-group">
                                                    <label for="inputcorreo" class="col-sm-2 control-label">Correo</label>
                                                    <div class="col-sm-10">
                                                        <input type="email" class="form-control" id="inputcorreo">
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label for="inputpassactual" class="col-sm-2 control-label">Contraseña Actual</label>
                                                    <div class="col-sm-10">
                                                        <input type="text" class="form-control" id="inputpassactual">
                                                    </div>
                                                </div>

                                                <div class="form-group">
                                                    <label for="inputpassnueva" class="col-sm-2 control-label">Nueva contraseña</label>
                                                    <div class="col-sm-10">
                                                        <input type="text" class="form-control" id="inputpassnueva">
                                                    </div>
                                                </div>

                                                <div class="form-group">
                                                    <label for="inputpasscnueva" class="col-sm-2 control-label">Confirmar</label>
                                                    <div class="col-sm-10">
                                                        <input type="text" class="form-control" id="inputpasscnueva">
                                                    </div>
                                                </div>

                                                
                                                
                                                <div class="box-footer">

                                                    @*<button type="submit" class="btn btn-info pull-right" onclick="javascript:Generarpasword()">Agregar</button>*@
                                                    <input type="submit" class="btn btn-info pull-right" value="Guardar Cambios" />
                                                </div><!-- /.box-footer -->

                                            </div>
                                    
                                 
                            </form>
                        </div><!-- /.box -->
                    </div>

                


            </div>

        </div>
    </div>

    <!-- Left col -->
    <section class="col-lg-7 connectedSortable"></section><!-- /.Left col -->
    <!-- right col (We are only adding the ID to make the widgets sortable)-->
    <section class="col-lg-5 connectedSortable"></section><!-- right col -->


</section><!-- /.content -->